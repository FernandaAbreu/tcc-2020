services:
  gymclub.api:
    build:
      context: /Users/fernandaabreu/tcc-2020/br.com.gymclub
      dockerfile: gymclub/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44321'
      ASPNETCORE_URLS: https://+:443;http://+:80
