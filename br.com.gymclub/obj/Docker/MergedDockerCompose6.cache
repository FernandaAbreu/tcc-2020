networks:
  backend:
    driver: bridge
services:
  api:
    build:
      context: /Users/fernandaabreu/tcc-2020/br.com.gymclub
      dockerfile: api/Dockerfile
    depends_on:
    - db
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:5556
    networks:
      backend: null
    ports:
    - published: 5556
      target: 5556
  db:
    environment:
      MYSQL_DATABASES: dbgymclub
      MYSQL_HOST: localhost
      MYSQL_MY_DATABASE: dbgymclub
      MYSQL_PASSWORD: root
      MYSQL_PORT: '33306'
      MYSQL_ROOT_PASSWORD: root
      MYSQL_USER: gymclubuser
    image: mysql:5.7
    networks:
      backend: null
    ports:
    - published: 3306
      target: 3306
    restart: always
    volumes:
    - dbdata:/var/lib/mysql:rw
    - /Users/fernandaabreu/tcc-2020/br.com.gymclub/_MySQL_Init_Script:/docker-entrypoint-initdb.d:rw
  ui:
    build:
      context: /Users/fernandaabreu/tcc-2020/br.com.gymclub
      dockerfile: ui/Dockerfile
    networks:
      backend: null
    ports:
    - published: 8520
      target: 80
version: '3.4'
volumes:
  dbdata: {}